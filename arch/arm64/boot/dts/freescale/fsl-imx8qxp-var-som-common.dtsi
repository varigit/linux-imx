	status = "okay";

	/* GPIO expander */
	pca9534:gpio@20 {
		compatible = "nxp,pca9534";
		reg = <0x20>;
		gpio-controller;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_pca9534>;
		interrupt-parent = <&gpio3>;
		interrupts = <4 IRQ_TYPE_EDGE_FALLING>;
