		#power-domain-cells = <0>;
		#address-cells = <1>;
		#size-cells = <0>;

		pd_dma2_chan9: PD_UART0_TX {
			reg = <SC_R_DMA_2_CH9>;
			power-domains =<&pd_dma2_chan8>;
